#!/bin/sh

#DEBHELPER#

ui=06univention-openvpn-server-uninstall.uinst

if [ "$1" = "remove" ]; then
	. /usr/share/univention-lib/base.sh
	call_unjoinscript "$ui"
	invoke-rc.d univention-directory-listener restart
fi

ucr unset security/packetfilter/package/univention-openvpn-server/tcp/38081/all
/etc/init.d/univention-firewall restart

exit 0
